{
	"compilerOptions": {
		"target": "es2015",
		"module": "es2015",
		"moduleResolution": "node",
		"skipLibCheck": true,
		"preserveConstEnums": true,
		"inlineSourceMap": true,
		"allowJs": true,
		"strict": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": false,
		"types": ["openui5"]
	},
	"include": [
		"./webapp/**/*"
	]
}
